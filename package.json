        "@types/jasminewd2": "^2.0.3",
        "@types/node": "^9.4.0",
        "jasmine": "~2.9.0",
        "jasmine-core": "~2.9.1",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-phantomjs-launcher": "^1.0.4",
